{
    "env": {
        "browser": false,
        "commonjs": true,
        "es6": true,
        "mocha":true,
        "node":true
    },
    "extends": [
        "airbnb-base"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "camelcase":"off",
      "no-await-in-loop":"off",
      "no-use-before-define":"off"
    }
}
