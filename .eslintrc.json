{
  "root": true,
  "env": {
    "commonjs": true,
    "node": true,
    "jest": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018
  },
  "rules": {
    "camelcase": "off",
    "no-await-in-loop": "off",
    "no-use-before-define": "off",
    "no-console": "off",
    "max-len": 0
  },
  "plugins": ["jest"]
}
